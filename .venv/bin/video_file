#!/home/shadowmorex/python_video_timelapse/.venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'python-video-timelapse','console_scripts','video_file'
__requires__ = 'python-video-timelapse'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('python-video-timelapse', 'console_scripts', 'video_file')()
    )
